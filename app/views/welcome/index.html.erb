<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>

<%= user_signed_in? ? ((image_tag avatar_url(current_user, 200)).html_safe + current_user.email) : "User" %>               
<% if user_signed_in? %>
<%= link_to('<i class="fa fa-key"></i>'.html_safe+'Logout', destroy_user_session_path, :method => :delete) %>
<% else %>
<%= link_to('<i class="fa fa-key"></i>'.html_safe+'Login', new_user_session_path)  %>  
<% end %>
